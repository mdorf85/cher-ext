{
  "manifest_version": 2,

  "name": "Cher With Friends",
  "description": "This extension will replace Share with Cher",
  "version": "1.0",
  "background": {
    "scripts": ["popup.js"]
  },
  "content_scripts":[ {
      "matches": ["https://www.facebook.com/*", "http://www.facebook.com/*"] ,
      "js": [ "popup.js" ]
    } ],
  "browser_action": {
   "default_icon": "tiny-cher19.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "http://*/"
   ],
  "web_accessible_resources": [
    "images/*.*"
  ]
}

